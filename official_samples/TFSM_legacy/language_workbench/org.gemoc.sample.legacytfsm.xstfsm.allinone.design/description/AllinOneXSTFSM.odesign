<?xml version="1.0" encoding="UTF-8"?>
<description:Group xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:description="http://www.eclipse.org/sirius/description/1.1.0" xmlns:description_1="http://www.eclipse.org/sirius/diagram/description/1.1.0" xmlns:style="http://www.eclipse.org/sirius/diagram/description/style/1.1.0" xmlns:tool="http://www.eclipse.org/sirius/description/tool/1.1.0" name="AllinOneXSTFSM" version="10.1.3.201511131800">
  <ownedViewpoints name="LegacyTFSM_AllInOne_Viewpoint">
    <ownedRepresentations xsi:type="description_1:DiagramDescription" name="TimeFSM_AllInOne" initialisation="true" domainClass="TimeFSM">
      <metamodel href="../../org.gemoc.sample.legacytfsm.xstfsm.xstfsm/model/XSTFSM.ecore#/"/>
      <defaultLayer name="Default">
        <nodeMappings name="LocalEvents" semanticCandidatesExpression="[self.localEvents/]" domainClass="FSMEvent">
          <style xsi:type="style:WorkspaceImageDescription" showIcon="false" sizeComputationExpression="-1" labelPosition="node" resizeKind="NSEW" workspacePath="/org.gemoc.sample.legacytfsm.xstfsm.allinone.design/icons/event_small.gif">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="LocalClock" semanticCandidatesExpression="[self.localClock/]" domainClass="FSMClock">
          <style xsi:type="style:WorkspaceImageDescription" sizeComputationExpression="-1" resizeKind="NSEW" workspacePath="/org.gemoc.sample.legacytfsm.xstfsm.allinone.design/icons/clock_local_small.png">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
          </style>
        </nodeMappings>
        <edgeMappings name="EventTransition" sourceMapping="//@ownedViewpoints[name='LegacyTFSM_AllInOne_Viewpoint']/@ownedRepresentations[name='TimeFSM_AllInOne']/@defaultLayer/@containerMappings[name='State']" targetMapping="//@ownedViewpoints[name='LegacyTFSM_AllInOne_Viewpoint']/@ownedRepresentations[name='TimeFSM_AllInOne']/@defaultLayer/@containerMappings[name='State']" targetFinderExpression="[self.target/]" sourceFinderExpression="[self.source/]" domainClass="Transition" useDomainElement="true">
          <style sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
            <centerLabelStyleDescription labelExpression="service:self.getLabel">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <containerMappings name="State" semanticCandidatesExpression="[self.ownedStates/]" domainClass="State">
          <style xsi:type="style:FlatContainerStyleDescription" arcWidth="50" arcHeight="50" borderSizeComputationExpression="1" roundedCorner="true" backgroundStyle="Liquid">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
            <foregroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_green']"/>
          </style>
          <conditionnalStyles predicateExpression="[self.eContainer().oclAsType(TimeFSM).initialState = self/]">
            <style xsi:type="style:FlatContainerStyleDescription" arcWidth="50" arcHeight="50" borderSizeComputationExpression="4" roundedCorner="true" backgroundStyle="Liquid">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
              <foregroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_green']"/>
            </style>
          </conditionnalStyles>
        </containerMappings>
      </defaultLayer>
      <additionalLayers name="Debug" activeByDefault="true">
        <toolSections name="Debug">
          <ownedTools xsi:type="tool:PopupMenu" name="Gemoc">
            <menuItemDescription xsi:type="tool:OperationAction" name="Debug" icon="/org.gemoc.executionframework.extensions.sirius/icons/debug_exc.gif">
              <view name="views"/>
              <initialOperation>
                <firstModelOperations xsi:type="tool:ExternalJavaAction" name="Debug" icon="/org.gemoc.executionframework.extensions.sirius/icons/debug_exc.gif" id="org.gemoc.execution.sequential.javaengine.ui.debug.sirius.action.gemocDebugAs"/>
              </initialOperation>
            </menuItemDescription>
            <menuItemDescription xsi:type="tool:OperationAction" name="Toggle breakpoint" icon="/org.gemoc.executionframework.extensions.sirius/icons/debug_exc.gif">
              <view name="views"/>
              <initialOperation>
                <firstModelOperations xsi:type="tool:ExternalJavaAction" name="Toggle Gemoc breakpoint" icon="/org.gemoc.executionframework.extensions.sirius/icons/breakpoint.gif" id="org.gemoc.execution.sequential.javaengine.ui.debug.sirius.action.GemocSequentialToggleBreakpointAction"/>
              </initialOperation>
            </menuItemDescription>
          </ownedTools>
        </toolSections>
        <decorationDescriptionsSet>
          <decorationDescriptions xsi:type="description_1:MappingBasedDecoration" name="Enabled breakpoint" position="WEST" decoratorPath="/org.gemoc.executionframework.extensions.sirius/icons/breakpoint_enabled.gif" preconditionExpression="service:self.hasEnabledBreakpoint"/>
          <decorationDescriptions xsi:type="description_1:MappingBasedDecoration" name="Disabled breakpoint" position="WEST" decoratorPath="/org.gemoc.executionframework.extensions.sirius/icons/breakpoint_disabled.gif" preconditionExpression="service:self.hasDisabledBreakpoint"/>
        </decorationDescriptionsSet>
        <customization>
          <vsmElementCustomizations xsi:type="description:VSMElementCustomization" predicateExpression="aql:self.isCurrentInstruction()">
            <featureCustomizations xsi:type="description:EReferenceCustomization" appliedOn="//@ownedViewpoints[name='LegacyTFSM_AllInOne_Viewpoint']/@ownedRepresentations[name='TimeFSM_AllInOne']/@defaultLayer/@containerMappings[name='State']/@style //@ownedViewpoints[name='LegacyTFSM_AllInOne_Viewpoint']/@ownedRepresentations[name='TimeFSM_AllInOne']/@defaultLayer/@containerMappings[name='State']/@conditionnalStyles.0/@style //@ownedViewpoints[name='LegacyTFSM_AllInOne_Viewpoint']/@ownedRepresentations[name='TimeFSM_AllInOne']/@defaultLayer/@nodeMappings[name='LocalClock']/@style //@ownedViewpoints[name='LegacyTFSM_AllInOne_Viewpoint']/@ownedRepresentations[name='TimeFSM_AllInOne']/@defaultLayer/@nodeMappings[name='LocalEvents']/@style" referenceName="borderColor" value="//@userColorsPalettes[name='XSTFSM%20Palette']/@entries[name='current%20instruction']"/>
            <featureCustomizations xsi:type="description:EReferenceCustomization" appliedOn="//@ownedViewpoints[name='LegacyTFSM_AllInOne_Viewpoint']/@ownedRepresentations[name='TimeFSM_AllInOne']/@defaultLayer/@edgeMappings[name='EventTransition']/@style" referenceName="strokeColor" value="//@userColorsPalettes[name='XSTFSM%20Palette']/@entries[name='current%20instruction']"/>
            <featureCustomizations xsi:type="description:EReferenceCustomization" appliedOn="//@ownedViewpoints[name='LegacyTFSM_AllInOne_Viewpoint']/@ownedRepresentations[name='TimeFSM_AllInOne']/@defaultLayer/@edgeMappings[name='EventTransition']/@style/@centerLabelStyleDescription //@ownedViewpoints[name='LegacyTFSM_AllInOne_Viewpoint']/@ownedRepresentations[name='TimeFSM_AllInOne']/@defaultLayer/@containerMappings[name='State']/@style //@ownedViewpoints[name='LegacyTFSM_AllInOne_Viewpoint']/@ownedRepresentations[name='TimeFSM_AllInOne']/@defaultLayer/@containerMappings[name='State']/@conditionnalStyles.0/@style //@ownedViewpoints[name='LegacyTFSM_AllInOne_Viewpoint']/@ownedRepresentations[name='TimeFSM_AllInOne']/@defaultLayer/@nodeMappings[name='LocalClock']/@style //@ownedViewpoints[name='LegacyTFSM_AllInOne_Viewpoint']/@ownedRepresentations[name='TimeFSM_AllInOne']/@defaultLayer/@nodeMappings[name='LocalEvents']/@style" referenceName="labelColor" value="//@userColorsPalettes[name='XSTFSM%20Palette']/@entries[name='current%20instruction']"/>
          </vsmElementCustomizations>
        </customization>
      </additionalLayers>
      <additionalLayers name="Animation">
        <decorationDescriptionsSet>
          <decorationDescriptions xsi:type="description_1:MappingBasedDecoration" name="Current State" position="SOUTH_EAST" decoratorPath="/org.gemoc.sample.legacytfsm.xstfsm.allinone.design/icons/cursor-3-24.png" preconditionExpression="[self.eContainer(TimeFSM).currentState = self/]" mappings="//@ownedViewpoints[name='LegacyTFSM_AllInOne_Viewpoint']/@ownedRepresentations[name='TimeFSM_AllInOne']/@defaultLayer/@containerMappings[name='State']"/>
          <decorationDescriptions xsi:type="description:SemanticBasedDecoration" name="EventOccured" position="CENTER" decoratorPath="/org.gemoc.sample.legacytfsm.xstfsm.allinone.design/icons/eventOccurred.png" preconditionExpression="service:self.hasBeenActivated" domainClass="FSMEvent"/>
        </decorationDescriptionsSet>
        <customization>
          <vsmElementCustomizations xsi:type="description:VSMElementCustomization" predicateExpression="service:self.hasBeenActivated">
            <featureCustomizations xsi:type="description:EReferenceCustomization" appliedOn="//@ownedViewpoints[name='LegacyTFSM_AllInOne_Viewpoint']/@ownedRepresentations[name='TimeFSM_AllInOne']/@defaultLayer/@edgeMappings[name='EventTransition']/@style" referenceName="strokeColor" value="//@userColorsPalettes[name='XSTFSM%20Palette']/@entries[name='activated%20instruction']"/>
            <featureCustomizations xsi:type="description:EReferenceCustomization" appliedOn="//@ownedViewpoints[name='LegacyTFSM_AllInOne_Viewpoint']/@ownedRepresentations[name='TimeFSM_AllInOne']/@defaultLayer/@containerMappings[name='State']/@style //@ownedViewpoints[name='LegacyTFSM_AllInOne_Viewpoint']/@ownedRepresentations[name='TimeFSM_AllInOne']/@defaultLayer/@containerMappings[name='State']/@conditionnalStyles.0/@style //@ownedViewpoints[name='LegacyTFSM_AllInOne_Viewpoint']/@ownedRepresentations[name='TimeFSM_AllInOne']/@defaultLayer/@nodeMappings[name='LocalClock']/@style //@ownedViewpoints[name='LegacyTFSM_AllInOne_Viewpoint']/@ownedRepresentations[name='TimeFSM_AllInOne']/@defaultLayer/@nodeMappings[name='LocalEvents']/@style" referenceName="borderColor">
              <value href="environment:/viewpoint#//@systemColors/@entries[name='red']"/>
            </featureCustomizations>
          </vsmElementCustomizations>
          <vsmElementCustomizations xsi:type="description:VSMElementCustomization">
            <featureCustomizations xsi:type="description:EAttributeCustomization" appliedOn="//@ownedViewpoints[name='LegacyTFSM_AllInOne_Viewpoint']/@ownedRepresentations[name='TimeFSM_AllInOne']/@defaultLayer/@nodeMappings[name='LocalClock']/@style" attributeName="labelExpression" value="aql:self.getClockLabel()"/>
          </vsmElementCustomizations>
        </customization>
      </additionalLayers>
    </ownedRepresentations>
    <ownedJavaExtensions qualifiedClassName="org.gemoc.sample.legacytfsm.xstfsm.allinone.design.services.XstfsmDebugServices"/>
    <ownedJavaExtensions qualifiedClassName="org.gemoc.sample.legacytfsm.xstfsm.allinone.design.services.XSTFSMServices"/>
    <ownedJavaExtensions qualifiedClassName="org.gemoc.sample.legacytfsm.xstfsm.allinone.design.services.TFSMServices"/>
    <ownedJavaExtensions qualifiedClassName="org.gemoc.sample.legacytfsm.xstfsm.allinone.design.services.XSTFSMAnimatorServices"/>
  </ownedViewpoints>
  <userColorsPalettes name="XSTFSM Palette">
    <entries xsi:type="description:UserFixedColor" red="255" green="255" blue="0" name="current instruction"/>
    <entries xsi:type="description:UserFixedColor" red="0" green="240" blue="255" name="activated instruction"/>
  </userColorsPalettes>
</description:Group>
